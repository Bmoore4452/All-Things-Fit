<main>
    <div class="title workout-page">
        <H2 class="workout-page">Instructor Led Workouts</H2>
    </div>
    <div>

        <div class="card-container">

            <div class="row card list">
                <div>
                    <p class="card-top-text"> List of Workouts</p>
                </div>
                <div>
                    <ul class="db-list">
                        {{#each instructors}}
                        <li class="db-selection" onclick="generateWorkout({{this.dataValues.id}})">
                            {{this.dataValues.title}}</li>
                        {{!--
                        <li class="db-selection">workout 2</li>
                        <li class="db-selection">workout 3</li> --}}
                        {{/each}}
                    </ul>
                </div>
            </div>
            <div class="row card main-content">
                <div>
                    <p class="card-top-text" id="workout-name">Workout Name here pulled in from database</p>
                </div>

                <div class=" card-image-1" id="workout-video">
                    <p>video</p>
                </div>
            </div>
        </div>
    </div>
</main>

<script src="/js/instructor.js"></script>